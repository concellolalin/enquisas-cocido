{% extends 'base.html.twig' %}

{% block title %}Enquisas{% endblock %}

{% block pagetitle %}Enquisas{% endblock %}

{% block body %}

    {{ form_start(form) }}

        <div class="form-group">
            {{ form_label(form.restaurante) }}
            {{ form_widget(form.restaurante, { 'attr': {'class': 'form-control'} }) }}
        </div>

        <div class="form-group">
            {{ form_label(form.ficheiro) }}
            {{ form_widget(form.ficheiro, { 'attr': {'class': 'form-control'} }) }}
        </div>

        <div class="form-group">
            {{ form_label(form.nome) }}
            {{ form_widget(form.nome, { 'attr': {'class': 'form-control'} }) }}
        </div>

        <div class="form-group">
            {{ form_label(form.procesada) }}
            {{ form_widget(form.procesada, { 'attr': {'class': 'form-control'} }) }}
        </div>

        <input type="submit" value="Crear" class="btn btn-warning" />
        &nbsp;&nbsp;&nbsp;
        <a href="{{ path('enquisa_index') }}" class="btn btn-primary">Listado</a>
    {{ form_end(form) }}



{% endblock %}
